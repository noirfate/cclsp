{
  "servers": [
    {
      "extensions": [
        "js",
        "ts",
        "jsx",
        "tsx"
      ],
      "command": [
        "npx",
        "--",
        "typescript-language-server",
        "--stdio"
      ],
      "rootDir": ".",
      "timeouts": {
        "initialization": 3000,
        "request": 30000,
        "diagnostics": {
          "maxWait": 5000,
          "idle": 300,
          "checkInterval": 50,
          "trigger": 3000
        }
      }
    },
    {
      "extensions": [
        "go"
      ],
      "command": [
        "gopls"
      ],
      "rootDir": ".",
      "timeouts": {
        "initialization": 5000,
        "request": 60000,
        "diagnostics": {
          "maxWait": 8000,
          "idle": 500,
          "checkInterval": 100,
          "trigger": 5000
        }
      }
    }
  ]
}